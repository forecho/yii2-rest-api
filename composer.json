{
    "name": "forecho/yii2-rest-api",
    "description": "Yii 2 REST API Project Template",
    "keywords": [
        "yii2",
        "framework",
        "rest",
        "basic",
        "project template"
    ],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/forecho/yii2-rest-api/issues?state=open",
        "source": "https://github.com/forecho/yii2-rest-api"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiithings/yii2-dotenv": "^1.0",
        "sizeg/yii2-jwt": "^2.0",
        "yiier/yii2-helpers": "^2.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.5",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "codeception/specify": "~0.4.6",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/module-filesystem": "^1.0.0",
        "codeception/module-yii2": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-rest": "^1.0.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "squizlabs/php_codesniffer": "^3.5.5",
        "phpro/grumphp": "^0.19.1",
        "mis/yii2-ide-helper": "^1.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "autoload": {
        "files": [
            "vendor/yiier/yii2-helpers/src/GlobalFunctions.php",
            "vendor/yiier/yii2-helpers/src/SupportFunctions.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
